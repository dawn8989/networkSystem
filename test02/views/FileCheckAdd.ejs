        <div class="body">
            <div class="content">
                <div class="page-header">
                    <div class="icon" style="visibility:hidden;">
                        <span class="ico-folder"></span>
                    </div>
                    <h2>文件完整性添加</h2>
                </div>

                <div class="row-fluid">
                    <div class="span12">
                        <div class="block">
                            <table class="table" cellpadding="0" cellspacing="0" width="100%">
                                <tbody>
                                <tr>
                                    <td width="10%" align="right">任务名称</td>
                                    <td width="10%">
                                        <input id="taskname" name="taskname" type="text" value=""/>
                                    </td>
                                    <td widht="2%"></td>
                                    <td width="10%">开始时间</td>
                                    <td width="10%">
                                        <input type="text" id="starttime" class="datetimepicker"/>
                                    </td>
                                    <td widht="2%"></td>
                                    <td width="10%">结束时间</td>
                                    <td width="10%">
                                        <input type="text" id="endtime" class="datetimepicker"/>
                                    </td>
                                    <td widht="2%"></td>
                                    <td width="10%">检查周期</td>
                                    <td width="10%">
                                        <select id="frequency" name="frequency">
                                            <option value="">请选择</option>
                                                <option value="2">2小时</option>
                                                <option value="4">4小时</option>
                                                <option value="6">6小时</option>
                                                <option value="8">8小时</option>
                                                <option value="10">10小时</option>
                                                <option value="12">12小时</option>
                                                <option value="14">14小时</option>
                                                <option value="16">16小时</option>
                                                <option value="18">18小时</option>
                                                <option value="20">20小时</option>
                                                <option value="22">22小时</option>
                                                <option value="24">24小时</option>
                                        </select>
                                    </td>
                                </tr>
                                <tr>
                                    <td width="10%" align="right">检查内容</td>
                                    <td width="10%">
                                        <select id="type" name="type">
                                            <option value="">请选择</option>
                                            <option value="1">本地转码文件</option>
                                            <option value="2">本地码流文件</option>
                                            <option value="3">数据中心转码文件</option>
                                        </select>
                                    </td>
                                    <td widht="2%"></td>
                                    <td width="10%">文件大小</td>
                                    <td width="10%">
                                        <input id="normalsize" name="normalsize" value="700MB" type="text"/>
                                    </td>
                                    <td widht="2%"></td>
                                    <td width="10%">有效范围</td>
                                    <td width="10%">
                                        <input id="sizethr" name="sizethr" value="10%" type="text"/>
                                    </td>
                                    <td widht="2%"></td>
                                    <td width="10%">文件缺失时间</td>
                                    <td width="10%">
                                        <input id="losttime" name="losttime" value="120分钟" type="text"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td width="10%" align="right">文件保存时间</td>
                                    <td width="10%">
                                        <select id="filesavedtime" name="filesavedtime">
                                            <option value="">请选择</option>
                                            <option value="2">2天</option>
                                            <option value="3">3天</option>
                                            <option value="4">4天</option>
                                            <option value="5">5天</option>
                                            <option value="6">6天</option>
                                            <option value="7">7天</option>
                                            <option value="8">8天</option>
                                            <option value="9">9天</option>
                                            <option value="10">10天</option>
                                            <option value="11">11天</option>
                                            <option value="12">12天</option>
                                            <option value="13">13天</option>
                                            <option value="14">14天</option>
                                            <option value="15">15天</option>
                                        </select>
                                    </td>
                                    <td widht="2%"></td>
                                    <td width="10%">
                                        <button class="btn btn-success">同步监测系统目录</button>
                                    </td>
                                    <td width="10%">
                                        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#addModal">添加</button>
                                    </td>
                                    <td widht="2%"></td>
                                    <td width="10%">
                                        <button class="btn btn-info">修改</button>
                                    </td>
                                    <td width="10%">
                                        
                                        
                                        <button class="btn btn-danger">删除</button>
                                    </td>
                                    <td widht="2%"></td>
                                    <td width="10%"></td>
                                    <td width="10%">
                                        <button class="btn btn">导入</button>
                                        <button class="btn btn">导出</button>
                                    </td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

                <!--任务列表 begin-->
                <div class="row-fluid">
                    <div class="span12">
                        <div class="block">
                            <div class="head blue">
                                <h2>检查范围</h2>
                            </div>

                            <div style="height: 450px; overflow-y: scroll; clear: right;" class="data-fluid">
                                <table class="table dtable lcnp" cellpadding="0" cellspacing="0" width="100%">
                                    <thead>
                                    <tr>
                                        <th><input type="checkbox" class="checkall"/></th>
                                        <th>检查内容</th>
                                        <th>文件路径</th>
                                        <th width="1%"></th>
                                    </tr>
                                    </thead>
                                    <tbody id="ContentList">
                                        
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <!--设备列表 end-->

            </div>
        </div>
    </div>
