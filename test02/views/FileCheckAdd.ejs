<body>
    <!--加载页面动画 begin-->
    <div id="loader"><img src="/img/loader.gif"/></div>
    <!--加载页面动画 end-->

    <!--添加检查目录弹出框 begin-->
    <!-- Modal -->
    <div class="modal fade" id="addModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">添加检查目录</h4>
                </div>
                <form id="addContent_form" method="post" action="addContent">
                    <div class="modal-body">
                        <div class="row-fluid">
                            <label class="span3">卫星</label>
                            <div class="span9">
                                <select id="satellite" name="satellite">
                                    <option value="">请选择</option>
                                    <option value="中星6A">中星6A</option>
                                    <option value="中星6B">中星6B</option>
                                </select>
                            </div>
                        </div>
                        <div class="row-fluid">
                            <label class="span3">转发器</label>
                            <div class="span9">
                                <select id="repeater" name="repeater">
                                    <option value="">请选择</option>
                                    <option value="11A">11A</option>
                                    <option value="S16">S16</option>
                                </select>
                            </div>
                        </div>
                        <div class="row-fluid">
                            <label class="span3">码流</label>
                            <div class="span9">
                                <select id="tsfile" name="tsfile">
                                    <option value="">请选择</option>
                                    <option value="新疆走出去">新疆走出去</option>
                                </select>
                            </div>
                        </div>
                        <div class="row-fluid">
                            <label class="span3">节目</label>
                            <div class="span9">
                                <select id="program" name="program">
                                    <option value="">请选择</option>
                                    <option value="宝贝家">宝贝家</option>
                                    <option value="车迷频道">车迷频道</option>
                                </select>
                            </div>
                        </div>
                        <div class="row-fluid">
                            <label class="span3">节目ID</label>
                            <div class="span9">
                                <input name="programID" type="text"/>
                            </div>
                        </div>
                        <div class="row-fluid">
                            <label class="span3">文件路径</label>
                            <div class="span9">
                                <input name="fileurl" type="text"/>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="submit" class="btn btn-primary" onclick="initContentTable()">添加</button>
                        <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
                    </div>
                </form>
                
            </div>
        </div>
    </div>
    <!--添加检查目录弹出框 end-->

    <!--wrapper begin-->
    <div class="wrapper">
